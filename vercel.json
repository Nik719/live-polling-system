{
  "version": 2,
  "builds": [
    {
      "src": "api/socket/[[...path]].ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/spa/**",
      "use": "@vercel/static"
    },
    {
      "src": "dist/server/node-build.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socket/[[...path]].ts",
      "headers": {
        "Connection": "upgrade",
        "Upgrade": "websocket"
      },
      "continue": true
    },
    {
      "src": "/api/(?<path>.*)",
      "dest": "/api/socket/[[...path]].ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/socket/[[...path]].ts": {
      "memory": 1024,
      "maxDuration": 60,
      "includeFiles": [
        "dist/server/**"
      ]
    }
  },
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/socket.io/:path*",
      "destination": "/api/socket/:path*"
    }
  ]
}
